clc,clear

% Load current directory
datastruct = dir;

% Convert to table
datatable = struct2table(datastruct);



for i = 3:107

curdatname = datatable(i,1); % Data name

a(i-2) = table2cell(curdatname); % Convert to cell

filename = char(a(i-2));     % Convert to string

A(i-2) = importdata(filename); % import data



% A(3).name(i-2) = filename
end

clearvars filename curdatname

A(4).name = []


for i = 3:107

curdatname = datatable(i,1); % Data name

a(i-2) = table2cell(curdatname); % Convert to cell


    A(i-2).name = a(i-2)
    
end

% A(3).name = 5

